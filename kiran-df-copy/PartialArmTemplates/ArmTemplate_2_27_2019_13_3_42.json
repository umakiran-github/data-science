{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "kiran-df-copy"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/linkedServiceHD_OnDemand')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "HDInsightOnDemand",
				"typeProperties": {
					"clusterType": "hadoop",
					"clusterSize": 3,
					"timeToLive": "00:05:00",
					"version": "3.6",
					"hostSubscriptionId": "98366d3a-1e49-4270-b9f9-8c0498b17038",
					"clusterResourceGroup": "KiranResourceGroup",
					"servicePrincipalId": "65b1fe42-7af6-4c47-8ba0-629da83e4150",
					"osType": "Linux",
					"tenant": "2b8eaa08-d28e-42fe-bd3b-c6162e2623fd",
					"clusterNamePrefix": "OnDemand",
					"clusterUserName": "admin",
					"clusterPassword": {
						"type": "SecureString",
						"value": "**********"
					},
					"clusterSshUserName": "sshuser",
					"clusterSshPassword": {
						"type": "SecureString",
						"value": "**********"
					},
					"servicePrincipalKey": {
						"type": "SecureString",
						"value": "**********"
					},
					"linkedServiceName": {
						"referenceName": "AzureBlobStorage1",
						"type": "LinkedServiceReference"
					},
					"headNodeSize": "",
					"dataNodeSize": "",
					"zookeeperNodeSize": ""
				},
				"connectVia": {
					"referenceName": "integrationRuntime2",
					"type": "IntegrationRuntimeReference"
				}
			}
		}
	]
}